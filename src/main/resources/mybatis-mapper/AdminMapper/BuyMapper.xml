<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.admin_fintech.mapper.BuyMapper">
    <select id="SelectBuy" resultType="com.example.admin_fintech.buy.dto.Buy">
        SELECT
            id,
            email,
            coinId,
            name,
            price,
            amount,
            total,
            indate
        FROM
            buy
    </select>

</mapper>