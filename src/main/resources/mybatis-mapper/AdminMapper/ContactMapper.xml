<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.admin_fintech.mapper.ContactMapper">
    <select id="SelectContact" resultType="com.example.admin_fintech.contact.dto.Contact">
        SELECT
        id,
        name,
        email,
        phone,
        message,
        indate
        FROM
        contact
    </select>

</mapper>